USE SCHEMA DEV_DM

create or replace TABLE DEV_DM.DM_ASSET  (
	ID VARCHAR(255),
	CONTACTID VARCHAR(255),
	ACCOUNTID VARCHAR(255),
	PARENTID VARCHAR(255),
	ROOTASSETID VARCHAR(255),
	PRODUCT2ID VARCHAR(255),
	ISCOMPETITORPRODUCT VARCHAR(255),
	CREATEDDATE VARCHAR(255),
	CREATEDBYID VARCHAR(255),
	LASTMODIFIEDDATE VARCHAR(255),
	LASTMODIFIEDBYID VARCHAR(255),
	SYSTEMMODSTAMP VARCHAR(255),
	ISDELETED VARCHAR(255),
	NAME VARCHAR(255),
	SERIALNUMBER VARCHAR(255),
	INSTALLDATE VARCHAR(255),
	PURCHASEDATE VARCHAR(255),
	USAGEENDDATE VARCHAR(255),
	STATUS VARCHAR(255),
	PRICE VARCHAR(255),
	QUANTITY VARCHAR(255),
	DESCRIPTION VARCHAR(255),
	OWNERID VARCHAR(255),
	STOCKKEEPINGUNIT VARCHAR(255),
	STREET VARCHAR(255),
	CITY VARCHAR(255),
	STATE VARCHAR(255),
	POSTALCODE VARCHAR(255),
	COUNTRY VARCHAR(255),
	LATITUDE VARCHAR(255),
	LONGITUDE VARCHAR(255),
	GEOCODEACCURACY VARCHAR(255),
	ADDRESS VARCHAR(255),
	LASTVIEWEDDATE VARCHAR(255),
	LASTREFERENCEDDATE VARCHAR(255),
	AIRSPRINT_COST__C VARCHAR(255),
	AIRSPRINT_OWNED__C VARCHAR(255),
	ANNUAL_HOURS__C VARCHAR(255),
	FROM_ASSET__C VARCHAR(255),
	HOURS_FOR_SALE__C VARCHAR(255),
	NOTICE_OF_SALE_DATE__C VARCHAR(255),
	OWNER_ASKING_PRICE__C VARCHAR(255),
	OWNERSHIP__C VARCHAR(255),
	PREVIOUS_OWNER__C VARCHAR(255),
	RETAIL_COST__C VARCHAR(255),
	SELLING_PRICE__C VARCHAR(255),
	WHOLESALE_VALUE__C VARCHAR(255),
	TOTAL_HOURS_FOR_SALE__C VARCHAR(255),
	AIRSPRINT_BOOK_VALUE_CDN__C VARCHAR(255),
	NOS_STATUS__C VARCHAR(255),
	GUARANTEED_RESPONSE_TIME__C VARCHAR(255),
	OPPORTUNITY__C VARCHAR(255),
	STATUS_REPORT__C VARCHAR(255),
	USAGE_TYPE__C VARCHAR(255),
	HISTORICAL_PRICING__C VARCHAR(255),
	LEASE_YEAR__C VARCHAR(255),
	ANNUAL_OR_MONTHLY_PAYER__C VARCHAR(255),
	AIRSPRINT_HOURS__C VARCHAR(255),
	CLIENT_OWNED_HOURS__C VARCHAR(255),
	TGT_UPDATE_DATE VARCHAR(255)
);
